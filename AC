FunPage.AddDropdown("None Chatted", {"Hey its me goku", "fly", "YOU'LL PAY FOR THAT", "Ultra Instinct Aura SFX", "MUI"}, function(Value)
	local LocalPlayer = game:GetService("Players").LocalPlayer
	local OldChar = LocalPlayer.Character
loadstring(game:HttpGet("https://raw.githubusercontent.com/ArcaneAw/boombox/main/AC", true))()
	function p(id)
		play(id, true, true)
	end
	function c(wrd)
		game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(wrd, "None")
	end
	if Value == "nasty" then
		c("so you real nasty, huh?!")
	else
		c(Value)
	end

    if Value == "Hey its me goku" then
		p(5612259294)
	    local Swing = Instance.new('Animation')
		Swing.AnimationId = "rbxassetid://4841397952"
		local ld = game:GetService('Players').LocalPlayer.Character:FindFirstChildWhichIsA('Humanoid'):LoadAnimation(Swing)
		ld:Play()
	elseif Value == "fly" then
		p(4471125043)
        loadstring(game:HttpGet('https://raw.githubusercontent.com/ArcaneAw/Shazam/main/AC'))()
		wait(2.4)
		p(838152983)
    elseif Value == "YOU'LL PAY FOR THAT" then
		p(4846014500)
    elseif Value == "Ultra Instinct Aura SFX" then
		p(1825389232)
    elseif Value == "MUI" then
		p(2261507666)
	end
end)
