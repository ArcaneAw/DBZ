local library =
    loadstring(game.HttpGet(game, "https://raw.githubusercontent.com/ArcaneAw/NEWUI/main/AC"))()

local Window = library:Window("DBZ")
function LS(link)
    loadstring(game:HttpGet(link))()
end

loadstring(game:HttpGet("https://raw.githubusercontent.com/ArcaneAw/boombox/main/AC", true))()
	function p(id)
		play(id, true, true)
	end
	
		assert(getrawmetatable)--anti cheat
		gmt = getrawmetatable(game)
		setreadonly(gmt, false)
		old = gmt.__namecall
		gmt.__namecall = newcclosure(function(self, ...)
			local args = {
				...
			}
			if tostring(args[1]) == "TeleportDetect" then
				return
			elseif tostring(args[1]) == "CHECKER_1" then
				return
			elseif tostring(args[1]) == "CHECKER" then
				return
			end
			return old(self, ...)
		end)

